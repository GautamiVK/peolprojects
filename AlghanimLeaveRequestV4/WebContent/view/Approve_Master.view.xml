<mvc:View controllerName="com.peol.view.Approve_Master" xmlns="sap.m"
	xmlns:shellfooter="sap.ushell.ui.footerbar" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
	<Page>
		<customHeader>
			<Bar>
				<contentLeft>
					<Button icon="sap-icon://nav-back" press="goBack" />
				</contentLeft>
				<contentMiddle>
					<Label text="{i18n>view.Master.apprej.title}"></Label>
					<Label id="count" text=""></Label>
				</contentMiddle>
			</Bar>
		</customHeader>
		<subHeader>
			<Bar>
				<contentLeft>
					<SearchField placeholder="{i18n>LR_SEARCH}" liveChange="handleSearch">
					</SearchField>
					<!-- <Button id="synchronize" icon="sap-icon://synchronize" press=""></Button> -->
				</contentLeft>
				<contentRight>

				</contentRight>
			</Bar>
		</subHeader>
		<List id="list" select="handleListSelect"></List>
		<footer>
			<Bar>
			</Bar>
		</footer>
	</Page>

</mvc:View>